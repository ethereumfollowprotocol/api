{
  "name": "efp-public-api",
  "version": "0.0.0",
  "description": "Ethereum Follow Protocol Public API",
  "repository": "github:ethereumfollowprotocol/api",
  "type": "module",
  "scripts": {
    "dev": "bunx wrangler dev --experimental-json-config --config=wrangler.json --latest=true",
    "dev:remote": "bun dev --remote",
    "build": "bunx wrangler deploy --dry-run --experimental-json-config --config=wrangler.json --outdir=dist --minify=true --latest=true",
    "generate-database-types": "bun ./scripts/generate-database-types.ts",
    "lint": "biome check --apply .",
    "format": "biome format . --write",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "clean": "rm -rf dist .wrangler node_modules"
  },
  "dependencies": {
    "consola": "^3.2.3",
    "drizzle-orm": "latest",
    "hono": "^3.8.3",
    "pretty-cache-header": "^1.0.0",
    "viem": "^2.0.0-beta.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.3.3",
    "@cloudflare/workers-types": "^4.20231016.0",
    "@supabase/supabase-js": "^2.38.4",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.9",
    "bun": "^1.0.7",
    "bun-types": "^1.0.7",
    "pg": "^8.11.3",
    "typescript": "^5.2.2",
    "wrangler": "^3.14.0"
  },
  "sideEffects": false,
  "homepage": "github:ethereumfollowprotocol/indexer/README.md"
}
